command /togglestaff:
	trigger:
		if {togglestaff.%player%} is 0:
			set {togglestaff.%player%} to 1
			send "&aYou can now see reports, requests and staff chat."
		else:
			if {toggestaff.%player%} is 1:
				set {togglestaff.%player%} to 0
				send "&cYou can no longer see reports, requests, and staff chat."
